<div class="page-header">
  <h3>Feedback #<%= @feedback.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Proposal</dt>
      <dd><%= @feedback.proposal_id %></dd>

      <dt>Body</dt>
      <dd><%= @feedback.body %></dd>

      <dt>User</dt>
      <dd><%= @feedback.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/feedbacks" class="btn btn-primary">
        Back
      </a>
      <a href="/feedbacks/<%= @feedback.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_feedback/<%= @feedback.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
